{{#isSketch}}
<div class="reportSection tableContainer">
    <h4>Habitats</h4>
    <table>
      <thead>
        <tr>
          <th>Habitat</th>
          <th>Percent within Sketch</th>
        </tr>
      </thead>
      <tbody>
        {{#sketch_habitats}}
          <tr>
            <td>{{HAB_TYPE}}</td>
            <td>{{PERC}}</td>
          </tr>
        {{/sketch_habitats}}
        <tfoot>
        <tr>
          <td colspan="5" class="paragraph" style="text-align:left;">
            <p style="margin-left:10px;">
              
            </p>
          </td>
        </tr>
      </tfoot>
    </table>
 </div>
{{/isSketch}}
{{^isSketch}}
<div class="reportSection tableContainer">
    <h4>Habitats</h4>
    {{#hasIntangibles}}
     <div class="list-header">The intangible zones within this proposal include the following habitats:</div>
    <table>
      <thead>
        <tr>
          <th>Habitat</th>
          <th>Percent within Sketch</th>
        </tr>
      </thead>
      <tbody>
