        </tr>
      </thead>
      <tbody>
        {{#extractive_habitats}}
          <tr>
            <td>{{HAB_TYPE}}</td>
            <td>{{PERC}}</td>
          </tr>
        {{/extractive_habitats}}
    </table>
    {{/hasExtractive}}
 </div>
{{/isSketch}}